{
  "name": "cybersecurity-threat-detector",
  "version": "1.0.0",
  "description": "Real-time cybersecurity threat detection system with real network monitoring",
  "main": "backend/server.js",
  "scripts": {
    "setup": "node backend/setup.js",
    "start": "node backend/setup.js && node backend/server.js",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "test": "cd backend && npm test",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules",
    "reset": "npm run clean && npm run install:all",
    "logs": "cd backend && npm run logs",
    "status": "echo \"Checking server status...\" && netstat -an | findstr :3001 && netstat -an | findstr :3002"
  },
  "keywords": [
    "cybersecurity",
    "threat-detection",
    "network-monitoring",
    "real-time",
    "security",
    "network-security",
    "threat-intelligence",
    "cyber-defense"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/cybersecurity-threat-detector.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/cybersecurity-threat-detector/issues"
  },
  "homepage": "https://github.com/yourusername/cybersecurity-threat-detector#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "optionalDependencies": {
    "mongodb": "^6.0.0"
  },
  "os": [
    "win32",
    "linux",
    "darwin"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
